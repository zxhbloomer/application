            <artifactId>spring-cloud-starter-netflix-turbine</artifactId>
        </dependency>
        <dependency>
            <groupId>com.jack.springcloud</groupId>
            <artifactId>common-client-parent</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>
